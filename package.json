{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc -p client/",
    "build:watch": "tsc -p client/ -w",
    "build:e2e": "tsc -p e2e/",
    "build:prod": "rimraf dist && node config/preProd.js && webpack --config config/webpack.prod.js --progress --profile --bail",
    "unittest": "node_modules/.bin/jasmine",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "client:dev": "concurrently \"tsc -p share/ -w\" \"npm run build:watch\" \"npm run serve\"",
    "server:dev": "concurrently \"tsc -p server/ -w\" \"node server/mainserver.js\"",
    "share:compile": "tsc -p share/ ",
    "server:compile": "tsc -p server/ ",
    "client:compile": "tsc -p client/ && echo ok || echo not ok",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./client/**/*.ts -t verbose"
  },
  "author": "thinhtran",
  "license": "ISC",
  "devDependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.46",
    "angular-in-memory-web-api": "~0.2.4",
    "awesome-typescript-loader": "^3.0.4",
    "bluebird": "^3.4.7",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "fs-sync": "^1.0.3",
    "html-webpack-plugin": "^2.28.0",
    "is-glob": "^3.1.0",
    "jasmine": "^2.5.3",
    "jasmine-core": "~2.4.1",
    "jquery": "^3.1.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "minimatch": "^3.0.3",
    "mv": "^2.1.1",
    "protractor": "~4.0.14",
    "recursive": "0.0.2",
    "recursive-readdir": "^2.1.1",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "systemjs": "^0.20.3",
    "ts-loader": "^2.0.0",
    "tslint": "^3.15.1",
    "typescript": "~2.0.10",
    "typings": "^2.1.0",
    "vue": "^2.1.10",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^2.6.1",
    "zone.js": "^0.7.4"
  },
  "dependencies": {
    "express": "^4.14.1",
    "phaser": "^2.6.2",
    "socket.io": "^1.7.2"
  }
}
